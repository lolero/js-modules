{
  "private": true,
  "name": "@js-modules/apps-travel-log-api-core",
  "description": "Apps Travel Log Api Core",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "modules:build": "pnpm --filter @js-modules/apps-travel-log-common-constants build && pnpm --filter @js-modules/apps-travel-log-common-constants-cjs build && pnpm --filter @js-modules/api-nest-keycloak-admin-client-cjs build && pnpm --filter @js-modules/common-utils-general build && pnpm --filter @js-modules/common-utils-general-cjs build && pnpm --filter @js-modules/api-nest-utils build && pnpm --filter @js-modules/api-nest-module-auth-keycloak build && pnpm --filter @js-modules/apps-travel-log-api-core-modules build",
    "dev:travel-log-api-core": "cross-env NODE_ENV=development nest start --watch",
    "build": "cross-env NODE_ENV=production nest build -p tsconfig.build.json",
    "prod": "node build/main",
    "typeorm": "pnpm build && cross-env NODE_ENV=development typeorm -d build/config/config.typeorm.dataSource.js"
  },
  "dependencies": {
    "@js-modules/api-nest-module-auth-keycloak": "workspace:^",
    "@js-modules/api-nest-utils": "workspace:^",
    "@js-modules/apps-travel-log-api-core-modules": "workspace:^",
    "@js-modules/apps-travel-log-common-constants-cjs": "workspace:^",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.12",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/swagger": "^11.1.0",
    "@nestjs/testing": "^11.0.12",
    "@nestjs/typeorm": "^11.0.0",
    "@types/cookie-session": "^2.0.49",
    "@types/express": "^5.0.1",
    "@types/lodash": "^4.17.16",
    "@types/supertest": "^6.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-session": "^2.1.0",
    "express": "^4.21.2",
    "keycloak-connect": "^26.1.1",
    "keycloak-js": "26.1.4",
    "lodash": "^4.17.21",
    "nest-keycloak-connect": "^1.10.1",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2"
  },
  "eslintConfig": {
    "rules": {
      "class-methods-use-this": "off",
      "no-useless-constructor": "off",
      "no-param-reassign": "off"
    }
  }
}
